{
  "name": "node-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "frontend": "http-server && react-scripts start",
    "backend": "json-server -w db.json",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "build": "webpack --mode production",
    "start": "npm run build && react-scripts start",
    "dev": "concurrently \"npm run build\" \"npm run backend\" \"npm run client\" \"npm run frontend\""
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "http-server": "^0.12.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.1.2",
    "css-loader": "^2.0.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "json-server": "^0.16.3",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "4.39.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.2.1"
  }
}
